<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource
        name="jdbc/agency" type="javax.sql.DataSource"
        maxTotal="100" maxIdle="30" maxWaitMillis="10000"
        url="${db.url}"
        driverClassName="${db.driver}"
        username="${db.username}" password="${db.password}"
    />
</Context>
